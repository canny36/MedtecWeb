html, body {background: url(../images/bg.jpg) repeat-x top left #6bbbd4; height:100%; margin: 0px; padding: 0px; font-family:Arial, Helvetica, sans-serif; color: #333; font-size:14px; }

#container {
min-height: 100%;
margin-bottom: -50px;
position: relative;
}

#footer {
height: 50px;
position: relative;
background:#0b3c4c; height:50px; line-height:50px; color:#fff; text-align:center; font-size:10px; margin-top:50px;
}

.clearfooter {
height: 50px;
clear: both;
}



a img {border: 0;}
p{padding: 0 0 12px 0;margin: 0;}
.clear{clear: both;}
.pagewidth { width:985px; margin:0px auto 0px auto;}

.name {font-size:14px; color:#ec2100; font-weight:bold;}
.boxbg {background:url(../images/boxbg.gif) repeat-y;}

.practice {margin-left:31px; float:left;}

#alerts { font-size:20px; color:#57b2cf; width:900px; margin:0px auto 0px auto; height:30px; text-align:center; line-height:30px; }
.pname {color:#567883; font-size:18px; }
.pnamered {color:#b64d00; font-size:18px;}

.loginfieldddl {background:url(../images/loginformbg.gif) repeat-x; border:#c1dce4 solid 1px; outline:none; height:23px; background:#e8f5ff; width:300px; padding-left:3px; font-size:14px overflow:hidden;}


/*login css*/

.loginhead { font-size:20px; color:#FFFFFF;}
.login {width:284px; margin:0px auto 0px auto;color:#567883; font-size:18px; text-align:left;}
.loginfield {background:url(../images/loginformbg.gif) repeat-x; border:#c1dce4 solid 1px; height:25px; line-height:25px; width:300px; padding-left:3px; font-size:14px;}

